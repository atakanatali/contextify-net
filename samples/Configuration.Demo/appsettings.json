{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Contextify": {
    "Logging": {
      "EnableDetailedLogging": true,
      "IncludePayloads": true
    },
    "Policy": {
      "DenyByDefault": false,
      "SourceVersion": "config-v1"
    }
  }
}
